ic void onClick(View v) {\r\n        Intent i;\r\n        switch (v.getId()){\r\n            case R.id.BitcoinInfo:\r\n                i = new Intent(getActivity(),Bitcoin.class);\r\n                startActivity(i);\r\n                break;\r\n\r\n        }\r\n    }\r\n}\r\n
